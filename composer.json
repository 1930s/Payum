{
    "name": "payum/payment-bundle",
    "type": "symfony-bundle",
    "description": "Integrate payum libs into symfony2 framework",
    "keywords": ["payment"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "payum/payum": "*",
        "symfony/framework-bundle": "2.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "payum/paypal-express-checkout-nvp": "dev-master@dev",
        "payum/be2bill": "dev-master@dev"
    },
    "suggest": {
        "payum/paypal-express-checkout-nvp": "If you want to use paypal express checkout nvp gateway",
        "payum/be2bill": "If you want to use be2bill gateway"
    },
    "autoload": {
        "psr-0": { "Payum\\PaymentBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Payum/PaymentBundle"
}